pipeline
{
    agent any
    stages{
        stage('clone'){
            steps{

                echo "clone...."
                git branch: 'main', url: 'https://github.com/minhvl1/test_jenkins.git'
                }
        }
         stage('Test') {
                   steps {
                            sh 'mvn -Dmaven.test.failure.ignore=true install'
                      }
    }
}
